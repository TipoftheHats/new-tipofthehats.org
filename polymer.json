{
  "entrypoint": "index.html",
  "shell": "src/elements/all-elements.html",
  "sources": [
	"src/**/*",
	"images/**/*",
	"bower.json"
  ],
  "extraDependencies": [
	"manifest.json",
	"bower_components/webcomponentsjs/webcomponents-lite.js"
  ],
  "lint": {
	"rules": [
	  "polymer-2"
	],
	"ignoreWarnings": [
	  "not-loadable",
	  "could-not-load"
	]
  },
  "builds": [{
	"bundle": true,
	"js": {"minify": true},
	"css": {"minify": true},
	"html": {"minify": true}
  }]
}
